<Window x:Class="komunikator.StartUpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:komunikator"
        mc:Ignorable="d"
        Title="Komunikator mietech.pl" Height="300" Width="359" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="80*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btClientRun" Content="KLIENT" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150" Height="40" Grid.Row="0" Click="btClientRun_Click"/>
        <Button x:Name="btnServerRun" Content="SERWER" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150" Height="40" Grid.Row="1" Click="btnServerRun_Click"/>

    </Grid>
</Window>
